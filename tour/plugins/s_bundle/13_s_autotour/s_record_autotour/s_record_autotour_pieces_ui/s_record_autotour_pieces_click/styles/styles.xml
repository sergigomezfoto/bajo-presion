<krpano>

    <style name="s_autotour_canvas_piece_clicked"
    type="text"
    width="300"
    height="350"
    align="left"
    bg="false"
    bgalpha="1"
    bgcolor="0x495667"
    parent="OVERLAY"      
    />
        <style name="s_acpc_index"
        type="text"
        html="1"
        css="color:#ffffff; font-size:11;"
        align="bottomright"
        bg="false"
        bgalpha="1"
        bgcolor="0x495667"
        parent="s_autotour_canvas_piece_clicked"  
        />
        <style name="s_acpc_picture"
        type="text"
        html="hola"
        width="get:s_config_spui_pieces.width"
        height="get:s_config_spui_pieces.height"
        bg="true"
        bgcolor="0x30cd12"
        bgalpha="1"
        css="color:#ffffff; font-size:14px;text-align: center;"
        enabled="true"
        parent="s_autotour_canvas_piece_clicked"
        padding="6"  
        />
            <style name="s_acpc_delete"
            type="text"
            bg="true"
            bgalpha="1"
            bgcolor="0x717171"
            bgcolor1="0x717171"
            bgcolor2="0xa0a0a0"
            bgcoloractive="0xb5b5b5"
            onhover="if(!s_autotour_recording,s_hover_action_rec_tools);"
            onout="if(!s_autotour_recording,s_out_action_rec_tools);"
            html="DELETE"  
            height="get:s_config_spui_buttons.height"
            width="get:s_config_spui_buttons.width"
            ox="calc:(s_config_spui_buttons.width * 1 ) + ( s_config_spui_buttons.separation * 2 )"
            oy="calc: s_config_spui_buttons.separation * 2"
            padding="6"
            vcenter="true"
            css="color:#ffffff;text-align:center;"
            align="lefttop" 
            alpha="1"  
            parent="s_acpc_picture" 

            />
            <style name="s_acpc_delete_move_time"
            type="text"
            bg="true"
            bgalpha="1"
            bgcolor="0x717171"
            bgcolor1="0x717171"
            bgcolor2="0xa0a0a0"
            bgcoloractive="0xb5b5b5"
            onhover="if(!s_autotour_recording,s_hover_action_rec_tools);"
            onout="if(!s_autotour_recording,s_out_action_rec_tools);"
            html="DELETE MOVE TIME"  
            height="get:s_config_spui_buttons.height"
            width="calc:s_config_spui_buttons.width + 60"
            ox="calc:(s_config_spui_buttons.width * 1 ) + ( s_config_spui_buttons.separation * 2 )"
            oy="calc:s_config_spui_buttons.height + (s_config_spui_buttons.separation * 4)"
            padding="6"
            vcenter="true"
            css="color:#ffffff;text-align:center;"
            align="lefttop" 
            alpha="1"  
            parent="s_acpc_picture" 

            />
            <style name="s_acpc_change_content"
            type="text"
            bg="true"
            bgalpha="1"
            bgcolor="0x717171"
            bgcolor1="0x717171"
            bgcolor2="0xa0a0a0"
            bgcoloractive="0xb5b5b5"
            onhover="if(!s_autotour_recording,s_hover_action_rec_tools);"
            onout="if(!s_autotour_recording,s_out_action_rec_tools);"
            html="CHANGE CONTENT"  
            height="get:s_config_spui_buttons.height"
            width="calc:s_config_spui_buttons.width + 60"
            ox="calc:(s_config_spui_buttons.width * 1 ) + ( s_config_spui_buttons.separation * 2 )"
            oy="calc:(s_config_spui_buttons.height ) * 3 + (s_config_spui_buttons.separation * 8)"
            padding="6"
            vcenter="true"
            css="color:#ffffff;text-align:center;"
            align="lefttop" 
            alpha="1"  
            parent="s_acpc_picture" 

            />
                <style name="s_text_input_def"
                    url="../../assets/s_textinput.js"
                    align="lefttop"
                    width="100"
                    height="20"
                    css="text-align:left;border: 1px solid #ccc; outline:1px solid lightgray;"
                    parent="s_acpc_picture" 
                    ox="calc:(s_config_spui_buttons.width * 1 ) + ( s_config_spui_buttons.separation * 2 )"
                    oy="calc:(s_config_spui_buttons.height ) * 2 + (s_config_spui_buttons.separation * 6)"
                    />
<style name="s_autotour_intertimes_canvas"
    type="text"
    width="520"
    height="80"
    align="left"
    bg="true"
    bgalpha="1"
    bgcolor="0x8f8f8f"
    handcursor="false"
    parent="OVERLAY"      
    />
<style name="s_autotour_intertimes_save"
    type="text"
        bg="true"
        bgalpha="1"
            bgcolor="0x717171"
            bgcolor1="0x717171"
            bgcolor2="0xa0a0a0"
            bgcoloractive="0xb5b5b5"
            onhover="if(!s_autotour_recording,s_hover_action_rec_tools);"
            onout="if(!s_autotour_recording,s_out_action_rec_tools);"
        html="SAVE"  
        height="get:s_config_spui_buttons.height"
        width="get:s_config_spui_buttons.width"
        ox="calc:s_config_spui_buttons.separation * 2 "
        oy="-6"
        padding="6"
        vcenter="true"
        css="color:#ffffff;text-align:center;"
        align="leftbottom" 
        alpha="1"  
        s_autotour_excluded="true"
        parent="s_autotour_intertimes_canvas"
    />
<style name="s_autotour_intertimes_close"
    type="text"
        bg="true"
        bgalpha="1"
            bgcolor="0x717171"
            bgcolor1="0x717171"
            bgcolor2="0xa0a0a0"
            bgcoloractive="0xb5b5b5"
            onhover="if(!s_autotour_recording,s_hover_action_rec_tools);"
            onout="if(!s_autotour_recording,s_out_action_rec_tools);"
        html="CLOSE"  
        height="get:s_config_spui_buttons.height"
        width="get:s_config_spui_buttons.width"
        ox="calc:(s_config_spui_buttons.width ) + ( s_config_spui_buttons.separation * 3 )"
        oy="-6"
        padding="6"
        vcenter="true"
        css="color:#ffffff;text-align:center;"
        align="leftbottom" 
        alpha="1"  
        s_autotour_excluded="true"
        parent="s_autotour_intertimes_canvas"
    />
</krpano>
