<krpano>
 <hotspot name="nico"
	url="assets/plaza_4.png"
    distorted="true"
    onloaded="calc_pos_from_hfov_yaw_pitch_roll(22.7428, 104.772, 5.16238, -23.9661); if(scene[get(xml.scene)].planar,if(zoom,set(zoom, false);););"
    handcursor="false"
    enabled="false"
    visible="true"
    alpha="0"
    zorder="10"
    bgcapture="false"
    capture="false"
/>

<!-- <<<<<<<<<<<<<<<<<< PREGUNTA 1 >>>>>>>>>>>>>>>>>> -->
<hotspot name="pregunta1" ath="-158.4090362835574" atv="-2.85770812462234" scale="1" rx="-9" ry="-4" rz="9.5"  style="speech_bubbles"
    s_position="bottom"
    s_photo="marta"
    s_user="@marta_ink371"
    s_date="12h"
    s_heart="true"
    s_content="¿Vomitar o sudar ayuda a pasar la borrachera?"
    onclick.addevent="accion_pregunta1();"
/>
<action name="accion_pregunta1">
    lookto(-115,-11,105,tween(easeinoutquad,0.6));
    delayedcall(0.1, 
        set(hotspot[nico].alpha,1);
    );
    

    delayedcall(0.5, 
            jscall(
                launchPopup('link1vomitar','80%','80%',
                    {
                        onClose: function(){krpano.call('accion_return_pregunta1()');}
                    }
                )
            );
    );
</action>
<action name="accion_return_pregunta1">
    <!-- tween(hotspot[pregunta1].alpha,0,0.1); 

    tween(hotspot[pregunta2].alpha,1,0.1);  -->
    looktohotspot(pregunta2,105,tween(easeinoutquad,0.6));
    set(hotspot[nico].alpha,0);
</action>
